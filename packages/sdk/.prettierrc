{
  "endOfLine": "auto",
  "overrides": [
    {
      "files": "*.ts",
      "options": {
        "semi": true,
        "printWidth": 120,
        "tabWidth": 4,
        "useTabs": false,
        "singleQuote": true,
        "trailingComma": "all",
        "bracketSpacing": true
      }
    }
  ]
}
